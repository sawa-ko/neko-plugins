{
	"name": "neko-plugins",
	"author": "@kaname-png",
	"license": "MIT",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"clean": "rimraf \"packages/**/dist\" \"packages/**/.turbo\"",
		"lint": "turbo run lint",
		"format": "prettier --write \"packages/**/{src,tests,scripts}/**/*.{mjs,ts,js}\"",
		"docs": "typedoc",
		"test": "turbo run test",
		"update": "yarn upgrade-interactive",
		"build": "turbo run build",
		"commit": "git-cz",
		"cz": "git-cz",
		"prepublishOnly": "yarn build",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"tslib": "^2.6.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.1",
		"@commitlint/config-conventional": "^18.4.0",
		"@favware/npm-deprecate": "^1.0.7",
		"@sapphire/eslint-config": "^5.0.2",
		"@sapphire/framework": "^4.7.2",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.0",
		"@swc/core": "^1.3.96",
		"@swc/jest": "^0.2.29",
		"@types/jest": "^29.5.8",
		"@types/node": "^20.9.0",
		"@types/rimraf": "^4.0.5",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"cz-conventional-changelog": "^3.3.0",
		"discord.js": "^14.14.1",
		"eslint": "~8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"jest-circus": "^29.7.0",
		"lerna": "^8.0.0",
		"lint-staged": "^15.1.0",
		"prettier": "^3.1.0",
		"pretty-quick": "^3.1.3",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"turbo": "^1.10.16",
		"typedoc": "^0.25.3",
		"typedoc-plugin-mdn-links": "^3.1.0",
		"typescript": "^5.2.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kaname-png/neko-plugins.git"
	},
	"engines": {
		"node": ">=v14.0.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"discordjs"
	],
	"bugs": {
		"url": "https://github.com/kaname-png/neko-plugins/issues"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.3.tgz"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@4.0.2"
}
